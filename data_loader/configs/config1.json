{
    "name": "Stock_CNN_LSTM",
    "n_gpu": 1,
    "arch": {
        "type": "CNNLSTMModel",
        "args": {}
    },
    "data_loader": {
        "type": "StockDataLoader",
        "args": {
            "data_dir": "./data_loader/processed_data/spy_processed.csv",
            "batch_size": 64,
            "shuffle": false,
            "validation_split": 0.2,
            "num_workers": 1,
            "input_columns": [
                "EMA_2_3",
                "EMA_2_5",
                "EMA_3_5"
            ],
            "target_columns": [
                "ROC_1"
            ],
            "window": 20
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse_loss",
    "metrics": [
        "regression_binary_pred",
        "f1_score"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 15,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 15,
        "tensorboard": true
    },
    "results": {
        "accuracy": 0.5550744398584906,
        "f-1 score": 0.7123075689192925,
        "mse_loss": 0.8442153592283527,
        "mse": 0.9259847793728113,
        "sharpe": 0.506488991540072
    }
}