{
    "name": "Stock_CNN_LSTM",
    "n_gpu": 1,
    "arch": {
        "type": "CNNLSTMModel",
        "args": {}
    },
    "data_loader": {
        "type": "StockDataLoader",
        "args": {
            "data_dir": "./data_loader/processed_data/spy_processed.csv",
            "batch_size": 64,
            "shuffle": false,
            "validation_split": 0.1,
            "num_workers": 1,
            "input_columns": [
                "SMA_2_3",
                "EMA_2_3",
                "SMA_2_5",
                "EMA_2_5",
                "SMA_2_10",
                "EMA_2_10",
                "SMA_5_10",
                "EMA_5_10"
            ],
            "target_columns": [
                "ROC_1"
            ],
            "window": 20
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse_loss",
    "metrics": [
        "regression_binary_pred",
        "f1_score"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 15,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 15,
        "tensorboard": true
    },
    "results": {
        "accuracy": 0.5627020474137931,
        "f-1 score": 0.7126685809252568,
        "mse_loss": 0.8069169875234365,
        "mse": 1.4777167458087206,
        "sharpe": 0.9354415389322815
    }
}